{
  "name": "mt",
  "displayName": "Mt",
  "description": "拦截AJAX请求,用于前端开发中修改请求数据、响应数据，用于接口问题排查、mock接口数据等场景。",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "rimraf extension/dev && run-p dev:web dev:js",
    "dev:web": "vite",
    "dev:js": "cross-env NODE_ENV=dev tsup  --config tsup.config.ts --watch src ",
    "build": "run-s clear build:web build:js",
    "build:web": "vite build",
    "build:js": "cross-env NODE_ENV=prod tsup  --config tsup.config.ts  --minify",
    "clear": "rimraf extension/prod ",
    "clear:dev": "rimraf extension/dev",
    "lint": "eslint --fix --ext .js,.vue,.ts ./"
  },
  "dependencies": {
    "vue-i18n": "^9.1.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.7.0",
    "@iconify/json": "^2.1.30",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/chrome": "^0.0.181",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.4.1",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.33",
    "@vueuse/core": "^8.2.6",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.31.0",
    "esno": "^0.14.1",
    "fs-extra": "^10.0.0",
    "kolorist": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tsup": "^6.4.0",
    "typescript": "^4.3.5",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vite-plugin-windicss": "^1.8.4",
    "vue": "^3.2.33",
    "vue-demi": "^0.12.5",
    "vue-global-api": "^0.4.1",
    "webext-bridge": "^4.0.0",
    "webextension-polyfill-ts": "^0.26.0",
    "windicss": "^3.5.1"
  }
}
